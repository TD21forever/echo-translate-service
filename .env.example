# Alibaba Cloud credentials
ALI_ACCESS_KEY_ID=your-access-key-id
ALI_ACCESS_KEY_SECRET=your-access-key-secret
ALI_NLS_APP_KEY=your-nls-app-key

# Optional overrides
SERVER_PORT=3000
LOG_LEVEL=info
ALI_TRANSLATION_TARGET_LANGUAGE=zh
ALI_TRANSLATION_SCENE=general

# Recognition settings
RECOGNITION_BUFFER_MAX_CHUNKS=16
RECOGNITION_MAX_START_SILENCE_MS=2000
RECOGNITION_MAX_END_SILENCE_MS=200
RECOGNITION_ENABLE_VOICE_DETECTION=false
RECOGNITION_ENABLE_SEMANTIC_SENTENCE_DETECTION=false
RECOGNITION_MIN_CHANGED_DELTA=4
RECOGNITION_SEND_SOURCE_IMMEDIATELY=true

# Advanced recognition parameters
RECOGNITION_MAX_SENTENCE_SILENCE_MS=800
RECOGNITION_ENABLE_DISFLUENCY=false
RECOGNITION_SPEECH_NOISE_THRESHOLD=0
# RECOGNITION_CUSTOMIZATION_ID=your-custom-model-id
# RECOGNITION_VOCABULARY_ID=your-vocabulary-id
